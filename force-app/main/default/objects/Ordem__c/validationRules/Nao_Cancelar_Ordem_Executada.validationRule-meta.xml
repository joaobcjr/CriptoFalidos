<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nao_Cancelar_Ordem_Executada</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( ISNULL(DataExecucao__c)) &amp;&amp;  ISPICKVAL( Status__c , &apos;Cancelada&apos;)</errorConditionFormula>
    <errorMessage>Não é possível cancelar ordens já executadas;</errorMessage>
</ValidationRule>
